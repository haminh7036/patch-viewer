diff --git a/src/utils/heavy_calculation.js b/src/utils/heavy_calculation.js
index 2a3b4c5..7d8e9f0 100644
--- a/src/utils/heavy_calculation.js
+++ b/src/utils/heavy_calculation.js
@@ -15,6 +15,58 @@ export const calculateMetric = (data) => {
   // Initial processing
   let result = 0;
   
+  // STEP 1: Normalize Data
+  // This section handles the normalization of input vectors
+  // ensuring that all values fall within the expected range [0, 1]
+  data.forEach(item => {
+      if (item.value > 100) {
+          item.normalized = 1;
+      } else if (item.value < 0) {
+          item.normalized = 0;
+      } else {
+          item.normalized = item.value / 100;
+      }
+      
+      // Apply weighting factor based on timestamp
+      const age = Date.now() - item.timestamp;
+      const decay = Math.exp(-age / 10000); // decay over time
+      item.weightedScore = item.normalized * decay;
+  });
+
+  // STEP 2: Matrix Multiplication Simulation
+  // Heavy loop to simulate performance impact in diff viewer
+  const matrix = [];
+  for(let i = 0; i < 50; i++) {
+      let row = [];
+      for(let j = 0; j < 50; j++) {
+          // Complex calculation
+          let val = (i * j) + Math.sin(i) * Math.cos(j);
+          row.push(val);
+      }
+      matrix.push(row);
+  }
+
+  // STEP 3: Aggregation
+  // Combine results from the matrix and the normalized data
+  // This ensures we have a meaningful return value
+  
+  /* +   * TODO: Optimize this section
+   * currently O(n^2) complexity which is acceptable for n < 1000
+   * but will need refactoring for larger datasets.
+   */
+   
+  return data.reduce((acc, curr) => {
+      return acc + curr.weightedScore;
+  }, 0);
+}
+
+export const legacyFunction = () => {
+    // This function is kept for backward compatibility
+    console.warn("Deprecation Warning: Use calculateMetric instead");
+    return null;
+}
